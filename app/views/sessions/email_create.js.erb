

var notice = "<%= escape_javascript(flash[:notice]) %>"
if (notice == "Signed in!")
	{
		window.location = "/dashboard"
	}
else if (notice == "Invalid email or password")
	{
		$(".email_signup_flash").html('<div id="flash_notice_email" class="alert alert-info"><%= escape_javascript(flash[:notice]) %></div>').hide().fadeIn('fast');
	}